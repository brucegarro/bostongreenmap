
<div id="carousel-example-generic" data-ride="carousel" class="carousel slide">
  <div class="row">
    <div class="floating-text">
      <h1>Find your space.</h1>
        <div class="btn-group select-width">
          <select data-placeholder="Select Your Neighborhood" class="parkfilter chzn-select form-control" id="neighborhoods">
            <option value="">All Neighborhoods</option>
            {{#each neighborhoods}}
              <option value="{{ id }}">{{ name }}</option>
            {{/each}}
          </select>
        </div>

        <div class="btn-group select-width">
          <select data-placeholder="Select Your Activity" class="parkfilter chzn-select form-control" id="facility__activity">
            <option value="">All Activities</option>
            {{#each activities}}
              <option value="{{ id }}">{{ name }}</option>
            {{/each}}
          </select>
        </div>
      <div class="gobutton pull-right" id="filter-select">GO</div>
    </div>  
  </div>

  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
    <li data-target="#carousel-example-generic" data-slide-to="1"></li>
    <li data-target="#carousel-example-generic" data-slide-to="2"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner" id="carousel-images-container">
    <div class="item active">
      <img class="carousel-image" src="static/img/4-JamaicaPondJP.JPG" alt="...">
      <div class="carousel-caption">
      </div>
    </div>
    <div class="item">
      <img class="carousel-image" src="static/img/13-OMalleyWaterfrontPark-Chelsea.JPG" alt="...">
      <div class="carousel-caption">
      </div>
    </div>
    <div class="item">
      <img class="carousel-image" src="static/img/Nkraft (Charles River Bike Path Sunset 1).jpg" alt="...">
      <div class="carousel-caption">
      </div>
    </div>
  </div>
</div>
